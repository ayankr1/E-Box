#include<stdio.h>
#include<string.h>
int main()
{

  char a[100],b[100];
  printf("Enter the word");
  scanf("%s",a);
  printf("\nEnter the word the student has typed");
  scanf("%s",b);
  if(strcmp(a,b))
  {
    printf("\nIt is wrong");
  }
  else
  {
    printf("\nIt is correct");
  }
  return 0;
}
